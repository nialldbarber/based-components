{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "standard-react",
    "plugin:prettier/recommended",
    "prettier/standard",
    "prettier/react",
    "plugin:@typescript-eslint/eslint-recommended",
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "17"
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react/jsx-fragments": 0
  },
  "plugins": [
    "jsx-a11y"
  ]
}
